      "description": "A button component",
      "properties": [
        {
          "name": "title",
          "type": "String",
          "description": "The title of button",
          "default": "Button"
        },
        {
          "name": "kind",
          "type": "String",
          "description": "The kind of button. This determines the button's styling.",
          "default": "primary"
        },
        {
          "name": "disabled",
          "type": "Boolean",
          "description": "Should the button be disabled?",
          "default": false
        },
        {
          "name": "href",
